pipeline {
    agent any
    tools {
        jdk 'java17'
        nodejs 'node16'
    }
    environment {
        SCANNER_HOME = tool 'sonar-scanner'
    }
    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }
        stage('Checkout from GITHUB') {
            steps {
                git url: https://github.com/Abbeydauda20/cicdReactGame.git', branch: 'main'
            }
        }
    }
}
